module Enum2

	type 
		T = ( A, B, C D E )
		T2 = ( G, H, I )
	var
		t : T
		
begin
	println("start Enum2")
	t := C

	case t of
	| A: println("A")
	| B..C: println("B")
	| D: println("D")
